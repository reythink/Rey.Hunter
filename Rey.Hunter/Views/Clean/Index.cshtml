@{
    this.ViewBag.Title = "Clean";
}

<div class="row">
    <div class="col-lg-3 col-md-4 col-sm-6">
        <div class="box box-info">
            <div class="box-header">
                <h3 class="box-title">Current Account: @this.ViewBag.Account.Id</h3>
            </div>
            <div class="box-body">
                <form method="post" action="">
                    <button name="clean" class="btn btn-primary pull-right"><i class="fa fa-trash"></i> Clean</button>
                </form>
            </div>
        </div>
    </div>
</div>